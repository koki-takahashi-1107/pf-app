<% if goods.present? %>
  <% goods.first(100).each do |good| %>
    <div class="list">
    <% if good["smallImageUrls"][0].present? %>
      <div class="image"><%= link_to (image_tag(good["smallImageUrls"][0], style: "width: 70px;height: 100px;display: inline-block;")), good.url %></div>
      <%end%>

      <div class="content-title">
        <h2><%= good.name %></h2>
      </div>
  <% end %>
<% end %>